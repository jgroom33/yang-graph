  }

  deviation "/oc-if:interfaces/"
          + "oc-if:interface/"
          + "oc-if:config/"
          + "ciena-ieee-lag:agg/"
          + "ciena-ieee-lag:mac-address" {
    deviate not-supported;
  }
}
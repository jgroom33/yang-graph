          + "ciena-ieee-lag:agg/"
          + "ciena-ieee-lag:member-ports/"
          + "ciena-ieee-lag:agg-port-partner-admin-system-ID" {
    deviate replace {
      config false;
    }
  }

  deviation "/oc-if:interfaces/"

  deviation /syslog:syslog/syslog:log-actions/syslog:console {
    deviate not-supported;
  }
  
  deviation /syslog:syslog/syslog:log-actions/syslog:buffer {
    deviate not-supported;
  }
  
  deviation /syslog:syslog/syslog:log-actions/syslog:file {
    deviate not-supported;
  }
    
  deviation /syslog:syslog/syslog:log-actions/syslog:remote/syslog:destination {
    deviate not-supported;
  }
  
  deviation /syslog:syslog/syslog:log-actions/syslog:terminal {
    deviate not-supported;
  }    
  
  deviation /syslog:syslog/syslog:log-actions/syslog:session {
    deviate not-supported;
  }

}